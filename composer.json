{
    "name": "capmega/user-crud",
    "description": "This is a library to create, delete and update an user",
    "keywords": ["laravel", "package", "Crud-User"],
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Robles",
            "email": "grobles@capmega.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/capmega17/userCrud.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Capmega\\UserCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Capmega\\UserCrud\\UserCrudServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Capmega\\UserCrud\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
